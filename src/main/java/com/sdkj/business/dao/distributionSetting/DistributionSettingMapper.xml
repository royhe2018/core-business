<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdkj.business.dao.distributionSetting.DistributionSettingMapper">
  <resultMap id="BaseResultMap" type="com.sdkj.business.domain.po.DistributionSetting">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="provence" jdbcType="VARCHAR" property="provence" />
    <result column="city" jdbcType="VARCHAR" property="city" />
    <result column="start_mileage" jdbcType="INTEGER" property="startMileage" />
    <result column="fee_type" jdbcType="INTEGER" property="feeType" />
    <result column="role_type" jdbcType="INTEGER" property="roleType" />
    <result column="distribution_method" jdbcType="INTEGER" property="distributionMethod" />
    <result column="amount" property="amount" />
  </resultMap>
 
  <select id="findSingleDistributionSetting" parameterType="java.util.Map" resultMap="BaseResultMap">
    select *
    from sdlh_distribution_setting
    <trim prefix="where" prefixOverrides="and">
    	<if test="id != null">and id = #{id}</if>
    	<if test="city != null">and city = #{city}</if>
    	<if test="roleType != null">and role_type = #{roleType}</if>
    	<if test="feeType != null">and fee_type = #{feeType}</if>
    </trim>
  </select>
  <select id="findDistributionSettingList" parameterType="java.util.Map" resultMap="BaseResultMap">
    select *
    from sdlh_distribution_setting
    <trim prefix="where" prefixOverrides="and">
    	<if test="id != null">and id = #{id}</if>
    	<if test="city != null">and city = #{city}</if>
    	<if test="roleType != null">and role_type = #{roleType}</if>
    	<if test="feeType != null">and fee_type = #{feeType}</if>
    </trim>
  </select>
</mapper>